<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('img/logoNoText.png')}}" rel="icon">
        <link href="{{ asset('img/logoNoText.png')}}" rel="apple-touch-icon">
        <!-- Bootstrap core CSS -->
        <link href="{{ asset('adminAssets/lib/bootstrap/css/bootstrap.min.css')}}" rel="stylesheet">
        <!--external css-->
        <link href="{{ asset('adminAssets/lib/font-awesome/css/font-awesome.css')}}" rel="stylesheet" />
        <link rel="stylesheet" type="text/css" href="{{ asset('adminAssets/css/zabuto_calendar.css')}}">
        <link rel="stylesheet" type="text/css" href="{{ asset('adminAssets/lib/gritter/css/jquery.gritter.css')}}" />
        <!-- Custom styles for this template -->
        <link href="{{ asset('adminAssets/css/style.css')}}" rel="stylesheet">
        <link href="{{ asset('adminAssets/css/style-responsive.css')}}" rel="stylesheet">
        <script src="{{ asset('adminAssets/lib/chart-master/Chart.js')}}"></script>
    {% endblock %}


</head>
<body>

{% include 'shared/_adminHeader.html.twig' %}

{% block body %}    {% endblock %}

{% include 'shared/_adminFooter.html.twig' %}

{% block javascripts %}
    <script src="{{ asset('adminAssets/lib/jquery/jquery.min.js')}}"></script>
    <script src="{{ asset('adminAssets/lib/bootstrap/js/bootstrap.min.js')}}"></script>
    <script class="include" type="text/javascript" src="{{ asset('adminAssets/lib/jquery.dcjqaccordion.2.7.js')}}"></script>
    <script src="{{ asset('adminAssets/lib/jquery.scrollTo.min.js')}}"></script>
    <script src="{{ asset('adminAssets/lib/jquery.nicescroll.js')}}" type="text/javascript"></script>
    <script src="{{ asset('adminAssets/lib/jquery.sparkline.js')}}"></script>
    <!--common script for all pages-->
    <script src="{{ asset('adminAssets/lib/common-scripts.js')}}"></script>
    <script type="text/javascript" src="{{ asset('adminAssets/lib/gritter/js/jquery.gritter.js')}}"></script>
    <script type="text/javascript" src="{{ asset('adminAssets/lib/gritter-conf.js')}}"></script>
    <!--script for this page-->
    <script src="{{ asset('adminAssets/lib/sparkline-chart.js')}}"></script>
    <script src="{{ asset('adminAssets/lib/zabuto_calendar.js')}}"></script>
    <script>
        $(document).ready(function(){
            $("#myInput").on("keyup", function() {
                var value = $(this).val().toLowerCase();
                $("#myTable tr").filter(function() {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
            });
        });
    </script>
{% endblock %}



</body>
</html>
