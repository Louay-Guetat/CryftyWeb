{% extends 'baseAdmin.html.twig' %}

{% block title %} Admin -ShowClient!{% endblock %}

{% block body %}

    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper site-min-height">
            <h3><i class="fa fa-angle-right"></i> Show</h3>
            <div class="row mt">
                <div class="col-lg-12">
                    <h1>Client detail</h1>
                    <p>First name : {{ C.firstname }}</p>
                    <p>Last name : {{ C.username }}</p>
                    <p>Email : {{ C.email }}</p>
                    <p>Phone number : {{ C.phonenumber }}</p>
                    <p>Age : {{ C.age }}</p>
                    <p>Address : {{ C.address }}</p>


                </div>
            </div>
        </section>
        <!-- /wrapper -->
    </section>
    <!-- /MAIN CONTENT -->
    <!--main content end-->

{% endblock %}

